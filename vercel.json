{
  "version": 2,
  "functions": {
    "api": {
      "maxDuration": {
        "default": 15,
        "Pro": 300,
        "Enterprise": 900
      }
    }
  },
  "builds": [
    {
      "src": "backend.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/backend.js"
    }
  ]
}
